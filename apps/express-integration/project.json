{
  "name": "express-integration",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/express-integration/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/express-integration",
        "main": "apps/express-integration/server.ts",
        "tsConfig": "apps/express-integration/tsconfig.app.json",
        "assets": ["apps/express-integration/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/express-integration/src/environments/environment.ts",
              "with": "apps/express-integration/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/node:node",
      "options": {
        "buildTarget": "express-integration:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "express-integration:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/express-integration/jest.config.ts"
      }
    }
  },
  "tags": []
}
